#!/usr/bin/gnuplot
set terminal epslatex standalone dashed dashlength 0.3 color font ',17'

set output "na11p_pad.tex"

set tmargin 0
set bmargin 0
set lmargin 0
set rmargin 0
set size 1.25,1.25
set key Left reverse samplen 2.2
set multiplot
#
set size 1,1
set origin 0.2,0.2
set xlabel  'Angle $\theta$ (deg.)'
set ylabel  'Photo-Angular Distribution'
set xrange [0:180]
set yrange [0:0.5]
set ytics
set label '\large Na$_{11}^+$, boost along $z$' at graph 0.5,0.9 center
set key at graph 0.45,0.6 center
offx=0.04
offy=0.09
# set arrow from graph 0.363-offx,0.4+offy to graph 0.363+offx,0.4+offy \
#     dt 2 lc rgb 'blue' lw 10 nohead
# set arrow from graph 0.363-offx,0.33+offy to graph 0.363+offx,0.33+offy \
#     dt 2 lc rgb 'dark-green' lw 10 nohead
# set arrow from graph 0.363-offx,0.265+offy to graph 0.363+offx,0.265+offy \
#     dt 2 lc rgb 'red' lw 10 nohead
# set label 'TDLDA' at 120,0.35 right
# set arrow from 120,0.35 to 165,0.35 lc 0 lw 4 front
# set arrow from 135,0.35 to 170,0.25 lc 0 lw 4 front
# set arrow from 135,0.35 to 170,0.16 lc 0 lw 4 front
# set label 'RTA' at 50,0.35 left
# set arrow from 50,0.35 to 12,0.35 lc 0 lw 4 front
# set arrow from 35,0.35 to 5,0.25 lc 0 lw 4 front
# set arrow from 35,0.35 to 5,0.155 lc 0 lw 4 front
#set arrow from 47,0.35 to 47,0.25 heads size screen 0.01,90 lc 0 lw 5
set label '$\left\{\begin{array}{l} \\ \\ \end{array}\right.$' at 45,0.3
set label 'RTA' at 40,0.305 center rotate by 90
set label '$\left.\begin{array}{l} \\ \\ \end{array}\right\}$' at 132,0.3
set label 'TDLDA' at 140,0.305 center rotate by -90
p 'na11p_pad.gnu' i 0 u 2:4 w l dt 2 lc rgb 'blue' lw 10 not,\
  'na11p_pad.gnu' i 1 u 2:4 w l lc rgb 'blue' lw 10 t '$E^*=3.06$ eV',\
  'na11p_pad.gnu' i 2 u 2:4 w l dt 2 lc rgb 'dark-green' lw 10 not,\
  'na11p_pad.gnu' i 3 u 2:4 w l lc rgb 'dark-green' lw 10 t '$E^*=5.44$ eV',\
  'na11p_pad.gnu' i 4 u 2:4 w l dt 2 lc rgb 'red' lw 10 not,\
  'na11p_pad.gnu' i 5 u 2:4 w l lc rgb 'red' lw 10 t '$E^*=8.50$ eV'

#
unset xtics
unset xlabel
unset ytics
unset ylabel
unset title
unset border
set key at graph 0.79,0.6 right
p NaN dt 2 lc rgb 'blue' lw 10  t ' ',\
  NaN dt 2 lc rgb 'dark-green' lw 10 t ' ',\
  NaN dt 2 lc rgb 'red' lw 10 t ' '
q
