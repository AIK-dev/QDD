@article{Mor05,
title={Quantum mechanical/molecular mechanical (QM/MM) Car-Parrinello
simulations in excited states},
author={M.-E. Moret and E. Tapavicza and L. Guidoni and U.F. R\"ohrig and 
M. Sulpizi and I. Tavernelli and U. Rothlisberger},
journal={Chimia},
volume={59},
year={2005},
pages={493},
Abstract={
    The combination of time-dependent density functional theory
    (TDDFT) for the description of excited states with a hybrid
    quantum mechanics/molecular mechanics (QM/MM) approach enables the
    study of photochemical processes in complex environments. Here, we
    present a short overview of recent applications of TDDFT/ MM
    approaches to a variety of systems including studies of the
    optical properties of prototypical organic and inorganic molecules
    in gas phase and solution, photoinduced electron transfer
    reactions in donor-bridge-acceptor complexes, and in situ
    investigations of the molecular mechanisms of photoactive
    proteins. The application of TDDFT/MM techniques to a wide range
    of systems enables an assessment of the current performance and
    limitations of these methods for the characterization of
    photochemical processes in complex systems.  Schweizerische
    Chemische Gesellschaft.
}
}

