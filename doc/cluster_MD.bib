@article{Kle97,
 atitle  =	 {Size-Dependent Plasmon Lifetimes and Electron-Phonon 
                  Coupling Time Constants for Surface Bound Na Clusters},
  author =	 {J-H Klein-Wiele and P Simon and  H.-G Rubahn },
  journal =	 prl,
  volume =	 {80},
  year =	 {1997},
  pages =	 {45},
  abstract = {
    In this paper we show that time-resolved analysis of second
    harmonic generation via femtosecond pump-probe studies can be used
    to extract simultaneously mean cluster radii, decay times tau sp
    of localized surface plasmon excitations, and electron-phonon
    coupling time constants tau ep in large Nan (n ~ 107) clusters,
    bound to insulator surfaces. From the dependence of plasmon
    lifetime on the mean cluster radius (5 <= r0 <= 55 nm) a maximum
    of tau sp = 10 fs at a mean radius of about 25 nm is
    observed. Additionally, for Na clusters with mean radii of 30 to
    50 nm and bound to a lithium fluoride surface the electron-phonon
    coupling time constant is found to be tau ep ~ 1.1 ps.}
}



