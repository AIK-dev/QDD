@article{Pav05a,
author={Y. Pavlyukh and W. H\"ubner},
title={Collective electronic excitations in clusters in the vicinity of metal surfaces},
journal={Appl. Phys. A},
volume={82},
year={2005},
page={67},
abstract={
We study the collective excitations of metal clusters approaching a
metal surface. Using a simple model for the frequency-dependent
dielectric constant \u03b5(\u03c9) and the multiple scattering method,
we numerically investigate the shift in the plasmon resonance due to
the coupling of the collective modes of the sphere with those of its
mirror image. Results of the model calculation are verified by means
of ab initio theory. As a prototype system we study Na9 + cluster on
the Cu(100) surface. The representation of the solid surface by a
cluster of several, typically 54 substrate atoms, is used in
combination with a high level configuration interaction (CI)
calculation.
},
}

