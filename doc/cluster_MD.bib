@article{Rei06c,
tag={NM},
atitle={Role of boundary conditions in dynamic studies of nuclear giant resonances},
author={P.--G.~Reinhard and P.~D.~Stevenson and D.~Almehed and J.~A.~Maruhn
 and M.~R.~Strayer},
journal={Phys. Rev. E},
volume={73},
year={2006},
pages={036709},
abstract={
Time-Dependent Hartree-Fock calculations of nuclear giant resonance
states are performed in the linear response domain, with emphasis on a
systematic analysis of techniques to cope with continuum effects in a
finite basis.  These techniques are the use of absorbing boundary
conditions in the time-dependent calculations, and
filtering in the subsequent linear response analysis of the spectra.
A judicious combination of these techniques results in spectra close
to those of true continuum calculations at a much reduced
computational cost.  The example of giant monopole resonances in
$^{16}$O nuclei is considered.
}
}

