@article{Mos02,
title={Supported magnetic nanoclusters: Soft landing of Pd clusters on a MgO surface},
author={M Moseler and H H\"akkinen and U Landman},
journal={Phys. Rev. Lett.},
volume={89 },
year={2002},
pages={176103},
Abstract={
    A first-principles investigation of soft-landing of PdN clusters
    (N=2-7 and 13) onto a MgO(001) surface containing a surface F
    center, FC(oxygen vacancy) was performed. The interaction between
    the Pd cluster and the FCC was found to evolve from an initial
    long-ranged attractive polarization into chemical bonding
    involving the localized FC electronic state located in the MgO
    band gap. For N\u22646 the cluster retained its gas-phase
    geometry, whereas adsorbed clusters with N=7 and 13 adapted to the
    underlying surface rocksalt structure. The interaction with the
    surface quenched the spin for clusters with N\u22643, retained the
    gas-phase triplet state (S=1) for \u2264N\u22647, and for N=13 the
    gas-phase nonet (S=4) transformed to a septet (S=3) state.
}
}

