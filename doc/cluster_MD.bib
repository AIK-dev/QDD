@article{Kue08,
 author= {S K\"ummel  and L Kronik},
atitle  = {Orbital-dependent density functionals: theory and applications},
 journal = {Rev. Mod. Phys.},
 volume = {80},
 pages = {3},
 year = {2008},
 abstract ={
This review provides a perspective of what is currently considered one
of the most promising routes in modern density functional theory -the
use of orbital-dependent functionals. We focus on four major themes:
The motivation for orbital-dependent functionals in terms of
limitations of semi-local functionals; The optimized effective
potential as a rigorous approach to incorporating orbital-dependent
functionals within the Kohn-Sham framework; The rationale, advantages,
and limitations of four popular classes of orbital-dependent
functionals; The use of orbital-dependent functionals for predicting
excited state properties. For each of these issues, both formal and
practical aspects are assessed.
}
}

