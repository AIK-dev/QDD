@article{Din12c,
  xtitle =   {Fingerprints of level depletion in photoelectron spectra of
  small Na clusters in the UV domain},
  author =  {P. M. Dinh and   S.~Vidal and P.-G. Reinhard and
  E. Suraud},
  journal = {New J. Phys.},
  volume =  {14},  
  year =    {2012},
  pages =   {063015},
  aurl={http://stacks.iop.org/1367-2630/14/063015},
  abstract= { 
In the framework of time-dependent density-functional theory (TDDFT), we study
 electronic emission from small Na clusters after irradiation by an
intense femtosecond laser pulse. Photoelectron Spectra (PES) are compared
with level depletion, that is, the electron loss in each
single-electron level. Laser frequencies in the UV range (from 9 to
19 eV) are explored. We find a clear correlation of the
peak areas in the photoelectron spectra with the state depletion. This
thus allows an experimental access to the ionization mechanism. This
correlation moreover demonstrates that one can find a physical
interpretation of the Kohn-Sham orbitals in PES, as in real-time TDDFT.
}
}


