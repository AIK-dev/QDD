@article{And04a,
title={Crossed beams pump and probe dynamics in metal clusters},
author={K. Andrae and P.--G. Reinhard and E. Suraud},
journal={Phys. Rev. Lett.},
volume={92},
year={2004},
pages={173402},
url={http://link.aps.org/doi/10.1103/PhysRevLett.92.173402},
abstract={
We investigate theoretically pump and probe dynamics in metal clusters
with crossed-polarization laser beams. We explore the relation between
the pronounced Mie plasmon resonance and the laser frequency.  The
resonance moves with the cluster radius and splits according to the
actual deformation. We demonstrate that probe pulses with different
(linear) polarization axes allow to resolve the global shape
oscillations of the cluster in monopole and quadrupole degrees of
freedom.
}
}



