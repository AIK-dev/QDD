@article{Mar14aR,
  xtitle =   {The TDHF Code Sky3D},
  author =  {J. A. Maruhn and P.-G. Reinhard and P. D. Stevenson and A. S. Umar},
  journal = {Comp. Phys. Comm.},
  volume =  {185},  
  year =    {2014},
  pages =   {2195},
  xurl={http://dx.doi.org/10.1016/j.cpc.2014.04.008}, 
  xnote={arXiv:1310.5946},
  abstract= {
The nuclear mean-field model based on Skyrme forces or related density
functionals has found wide-spread application for the description of
nuclear ground states, collective vibrational excitations, and
heavy-ion collisions. The code Sky3D solves the static or dynamic
equations in a three-dimensional Cartesian mesh with periodic or
isolated boundary condtions and no further symmetry
assumptions. Pairing can be included in the BCS approximation. The
code is implemented with a view to easy modification for including
additional physics or special analysis of the results.
 }
}


